@model List<printingsystem.Models.Files.Files>
@{
    ViewData["Title"] = "Admin Panel";
    Random rnd = new Random();
}
@inject Microsoft.AspNetCore.Hosting.IWebHostEnvironment hostingEnvironment

<div class="card mb-3 admin-card">
    <div class="row g-0 admin-row">
        <div class="col-md-4 quicklinks_wrapper">
            <ul class="list-group text-center quicklinks">
                <li class="list-group-item text-uppercase">
                    <a asp-controller="Admin" asp-action="Index" class="h5 text-dark">Dashboard</a>
                </li>
                <li class="list-group-item text-uppercase">
                    <a asp-controller="Admin" asp-action="print" class="h5 text-dark">
                        Print Anywhere
                    </a>
                </li>
                <li class="list-group-item text-uppercase">
                    <a asp-controller="Admin" asp-action="Reports" class="h5 text-dark">Reports</a>
                </li>
            </ul>
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Date</th>
                            <th>Papers Used</th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var data in Model)
                        {
                            <tr>
                                <td></td>
                                <td>@ViewBag.total</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>